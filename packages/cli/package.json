{
  "name": "@mcpscript/cli",
  "version": "0.1.4",
  "description": "Command-line interface to run and compile MCP scripts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "mcps": "bin/mcps.mjs"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage --passWithNoTests"
  },
  "dependencies": {
    "@mcpscript/runtime": "file:../runtime",
    "@mcpscript/transpiler": "file:../transpiler",
    "@mishieck/ink-titled-box": "^0.3.0",
    "commander": "^14.0.2",
    "ink": "^6.5.0",
    "ink-text-input": "^6.0.0",
    "react": "^19.2.0"
  },
  "author": "Kun Chen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcpscript/mcpscript.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/mcpscript/mcpscript#readme",
  "bugs": {
    "url": "https://github.com/mcpscript/mcpscript/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "cli",
    "command-line",
    "scripting",
    "ai",
    "llm",
    "agents",
    "workflow",
    "automation",
    "transpiler",
    "compiler"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "devDependencies": {
    "@types/react": "^19.2.5"
  }
}
