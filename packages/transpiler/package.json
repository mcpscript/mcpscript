{
  "name": "@mcpscript/transpiler",
  "version": "0.1.0",
  "description": "Transpiler and parser for MCP Script using tree-sitter grammar - converts .mcps source code to executable JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:grammar && tsc",
    "build:grammar": "cd grammar && tree-sitter generate && node-gyp rebuild",
    "test": "npm run test:grammar && npm run test:unit",
    "test:grammar": "cd grammar && tree-sitter test",
    "test:unit": "vitest run",
    "dev": "tsc --watch",
    "clean": "rm -rf dist && rm -rf grammar/src && rm -rf grammar/build"
  },
  "dependencies": {
    "@mcpscript/runtime": "file:../runtime",
    "tree-sitter": "^0.25.0"
  },
  "author": "Kun Chen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/atinylittleshell/mcp-script.git",
    "directory": "packages/transpiler"
  },
  "homepage": "https://github.com/atinylittleshell/mcp-script#readme",
  "bugs": {
    "url": "https://github.com/atinylittleshell/mcp-script/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "transpiler",
    "compiler",
    "parser",
    "tree-sitter",
    "scripting-language",
    "codegen",
    "ast",
    "syntax"
  ]
}
