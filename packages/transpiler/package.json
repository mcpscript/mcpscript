{
  "name": "@mcpscript/transpiler",
  "version": "0.1.9",
  "description": "Transpiler and parser for MCP Script using tree-sitter grammar - converts .mcps source code to executable JavaScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run build:grammar && tsc",
    "build:grammar": "cd grammar && tree-sitter generate && node-gyp rebuild",
    "install": "test -f ../../package.json && exit 0 || (cd grammar && node-gyp-build)",
    "test": "npm run test:grammar && npm run test:unit",
    "test:grammar": "cd grammar && tree-sitter test",
    "test:unit": "vitest run",
    "dev": "tsc --watch",
    "clean": "rm -rf dist && rm -rf grammar/src && rm -rf grammar/build"
  },
  "dependencies": {
    "@mcpscript/runtime": "^0.1.9",
    "tree-sitter": "^0.25.0"
  },
  "author": "Kun Chen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcpscript/mcpscript.git",
    "directory": "packages/transpiler"
  },
  "homepage": "https://github.com/mcpscript/mcpscript#readme",
  "bugs": {
    "url": "https://github.com/mcpscript/mcpscript/issues"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "transpiler",
    "compiler",
    "parser",
    "tree-sitter",
    "scripting-language",
    "codegen",
    "ast",
    "syntax"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
